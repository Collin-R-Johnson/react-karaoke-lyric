{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "func-names": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-filename-extension": ["error", { "extensions": [".js"] }],
    "react/prefer-stateless-function": "off"
  },
  "plugins": [
    "react"
  ]
}
